{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_DEV_HOST:-10.0.0.1};Port=${DB_DEV_PORT:-5432};Database=${DB_DEV_NAME:-cms};Username=${DB_DEV_USER:-cmssystem};Password=${DB_DEV_PASSWORD};SSL Mode=prefer;Pooling=true;Minimum Pool Size=5;Maximum Pool Size=100;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Host": "10.0.0.1",
    "Port": 5432,
    "Name": "cms",
    "User": "cmssystem",
    "SslMode": "prefer",
    "ConnectionPool": {
      "MinSize": 5,
      "MaxSize": 100
    },
    "Retry": {
      "MaxRetryCount": 3,
      "MaxRetryDelay": 5,
      "EnableRetryOnFailure": true
    },
    "CommandTimeout": 60
  },
  "VPN": {
    "Required": true,
    "Type": "WireGuard",
    "NetworkRange": "10.0.0.0/24"
  }
}
