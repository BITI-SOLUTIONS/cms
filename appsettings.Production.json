{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_PROD_HOST:-10.0.0.1};Port=${DB_PROD_PORT:-5432};Database=${DB_PROD_NAME:-cms};Username=${DB_PROD_USER:-cmssystem};Password=${DB_PROD_PASSWORD};SSL Mode=require;Pooling=true;Minimum Pool Size=10;Maximum Pool Size=200;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Host": "10.0.0.1",
    "Port": 5432,
    "Name": "cms",
    "User": "cmssystem",
    "SslMode": "require",
    "ConnectionPool": {
      "MinSize": 10,
      "MaxSize": 200
    },
    "Retry": {
      "MaxRetryCount": 3,
      "MaxRetryDelay": 5,
      "EnableRetryOnFailure": true
    },
    "CommandTimeout": 60
  },
  "VPN": {
    "Required": true,
    "Type": "WireGuard",
    "NetworkRange": "10.0.0.0/24"
  }
}
